{
  "name": "orchestration-agent",
  "version": "1.0.0",
  "description": "AI Agent Orchestrator for Port Flow - coordinates booking and slots availability agents",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "test": "vitest",
    "test:unit": "vitest run --reporter=verbose",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@ai-sdk/mistral": "^1.2.8",
    "@trpc/client": "^11.0.0-rc.446",
    "@trpc/server": "^11.0.0-rc.446",
    "ai": "^4.3.16",
    "booking-agent": "file:../booking-agent",
    "dotenv": "^16.4.5",
    "slots-availability-agent": "file:../slots-availability-agent",
    "superjson": "^2.2.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
